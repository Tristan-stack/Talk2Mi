#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/www/Talk2Mi/backend/node_modules/.pnpm/is-base64@1.1.0/node_modules/is-base64/bin/node_modules:/mnt/c/www/Talk2Mi/backend/node_modules/.pnpm/is-base64@1.1.0/node_modules/is-base64/node_modules:/mnt/c/www/Talk2Mi/backend/node_modules/.pnpm/is-base64@1.1.0/node_modules:/mnt/c/www/Talk2Mi/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/www/Talk2Mi/backend/node_modules/.pnpm/is-base64@1.1.0/node_modules/is-base64/bin/node_modules:/mnt/c/www/Talk2Mi/backend/node_modules/.pnpm/is-base64@1.1.0/node_modules/is-base64/node_modules:/mnt/c/www/Talk2Mi/backend/node_modules/.pnpm/is-base64@1.1.0/node_modules:/mnt/c/www/Talk2Mi/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../is-base64@1.1.0/node_modules/is-base64/bin/is-base64" "$@"
else
  exec node  "$basedir/../../../../../is-base64@1.1.0/node_modules/is-base64/bin/is-base64" "$@"
fi
